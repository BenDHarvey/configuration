load_module /usr/lib/nginx/modules/ngx_stream_module.so;

events {}

stream {
      upstream k3s_servers {
      {% for host in k3s_nodes %}
        server {{ hostvars[host].ansible_default_ipv4.address }}:6443;
      {% endfor %}
      }

      server {
        listen 6443;
        proxy_pass k3s_servers;
      }
}
